    <script>
      const queryString = window.location.search;
      console.log(queryString);
      const urlParams = new URLSearchParams(queryString);
      var iframe = document.getElementById("divisio");
      var apphttp = queryString.replace("?", "");
      var confighttp = "https://script.google.com/macros/s/AKfycbwDcFyPQFV3B0bzeRxGU9yaTWhbA3PyR3SQZOQ1KEE5cU08SJb5QaOOfuXxwfVnuASk/exec?page=nouIndex&idfull=1HDQ6YdsA5FnzHaHFeOlYXPEIDCLRpq34rm078oFqRMU";

      window.addEventListener("load", function () {
        urlParams ? vesURL(apphttp) : vesURL(confighttp);
      });


      function vesURL(url) {
        iframe.src = url;
        
      }



    </script>
